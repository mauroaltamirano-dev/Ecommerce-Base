{
  "name": "backend-iii",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node ./index.js --mode=prod",
    "dev": "node --watch ./index.js",
    "test": "node ./index.js --mode=test",
    "artillery-pocos": "artillery quick --count 500 --num 2 \"http://localhost:8090/api/sumar/pocos\" -o ./test/artillery/pocos.json",
    "artillery-muchos": "artillery quick --count 500 --num 2 \"http://localhost:8090/api/sumar/muchos\" -o ./test/artillery/muchos.json",
    "artillery-flujo": "artillery run ./test/artillery/flow/config.yaml -o ./test/artillery/flow/flujo.json",
    "artillery-reporte": "artillery run ./test/artillery/flow/config.yaml --record --key a9_f582haxwbm5lpygzz0oo27dn7nbc3ini -o ./test/artillery/flow/config.json",
    "build-image": "docker build -t ecommerce-server .",
    "run-image": "docker run --rm -d --name ecommerce-dev  -p 9090:9090 ecommerce-server",
    "retag-image": "docker tag ecommerce-server tukics/ecommerce:1.0.1",
    "retag-latest": "docker tag ecommerce-server tukics/ecommerce:latest",
    "push-latest": "docker push tukics/ecommerce:latest",
    "push-image": "docker push tukics/ecommerce:1.0.1",
    "apply-manifest": "kubectl apply -f manifest.yaml",
    "mocha": "mocha ./test/mocha",
    "chai": "mocha ./test/chai/cart.test.js",
    "super": "mocha ./test/supertest"
  },
