    "cookie-parser": "^1.4.7",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-compression": "^1.0.2",
    "express-handlebars": "^8.0.3",
    "express-handlebars-sections": "^1.0.1",
    "handlebars": "^4.7.8",
    "handlebars-helpers": "^0.10.0",
